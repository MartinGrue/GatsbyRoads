{
  "compileOnSave": false,
  "compilerOptions": {
    "jsx": "react",
    "noImplicitAny": false,
    "module": "commonjs",
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "baseUrl": "./",
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
